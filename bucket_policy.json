{
   "Statement": [
     {
       "Sid": "AWSConfigBucketPermissionsCheck",
       "Effect": "Allow",
       "Principal": {
         "Service": "config.amazonaws.com",
         "AWS": "arn:aws:iam::240290037948:role/AWSConfigCustomServiceRole"
       },
       "Action": "s3:GetBucketAcl",
       "Resource": "arn:aws:s3:::configbucket-yz0988322",
       "Condition": {
         "StringEquals": {
           "AWS:SourceAccount": "240290037948"
         }
       }
     },
     {
       "Sid": "AWSConfigBucketExistenceCheck",
       "Effect": "Allow",
       "Principal": {
         "Service": "config.amazonaws.com",
         "AWS": "arn:aws:iam::240290037948:role/AWSConfigCustomServiceRole"
       },
       "Action": "s3:ListBucket",
       "Resource": "arn:aws:s3:::configbucket-yz0988322",
       "Condition": {
         "StringEquals": {
           "AWS:SourceAccount": "240290037948"
         }
       }
     },
     {
       "Sid": "AWSConfigBucketDelivery",
       "Effect": "Allow",
       "Principal": {
         "Service": "config.amazonaws.com",
         "AWS": "arn:aws:iam::240290037948:role/AWSConfigCustomServiceRole"
       },
       "Action": "s3:PutObject",
       "Resource": "arn:aws:s3:::configbucket-yz0988322/*",
       "Condition": {
         "StringEquals": {
           "s3:x-amz-acl": "bucket-owner-full-control",
           "AWS:SourceAccount": "240290037948"
         }
       }
     }
   ]
 }
